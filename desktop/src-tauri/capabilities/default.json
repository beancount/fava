{
  "$schema": "https://schema.tauri.app/config/2/capabilities",
  "identifier": "default",
  "description": "Default capabilities for rustfava desktop app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/rustfava-server",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "shell:allow-kill"
  ]
}
