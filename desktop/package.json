    "tauri:build": "bun run ensure-rustledger && tauri build"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "@xterm/xterm": "^5.5.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "tauri-pty": "^0.2"
  }
